{
  "name": "60plus",
  "version": "0.1.0",
  "description": "webpack/express boilerplate",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "DEBUG=debug:* NODE_ENV=development node server.js",
    "postinstall": "node heroku",
    "test": "echo \"Error: no test specified\" && exit 1",
    "local": "NODE_ENV=production npm build && NODE_ENV=production PORT=8080 npm start",
    "deploy": "npm run build && git add . && git commit -m 'release build' && git push heroku master",
    "test-deploy": "npm run build && git add . && git commit -m 'test build' && git push heroku-test master",
    "start": "node server.js",
    "log": "git log --pretty=format:'%h - %an, %ar : %s' > commits.txt"
  },
  "author": "Jan Jorissen",
  "license": "MIT",
